@{ ViewData["Title"] = "Application Page";                Layout = "~/Views/Shared/_AdminLayout.cshtml";}<div class="container-fluid">    <h1 class="text-center">Admin Dashboard</h1></div>@if (Model != null && Model.Count() > 0){    <table class="table-bordered table-striped" style="width:100%;">        <thead>            <tr>                <th>FirstName</th>                <th>UserName</th>                <th>Email</th>                <th>PhoneNumber</th>                <th>Password</th>                <th>ConfirmPassword</th>            </tr>        </thead>        @foreach (var obj in Model)        {            <tbody>                <tr>                    <td>@obj.FirstName</td>                    <td>@obj.username</td>                    <td>@obj.Email</td>                    <td> @obj.PhoneNumber</td>                    <td>@obj.Password</td>                    <td>@obj.ConfirmPassword</td>                    <td class="text-center">                        <div class="w-75 btn-group" role="group">                            <a asp-controller="MyRegistrationForm" asp-route-Id="@obj.Id" asp-action="Edit" class="btn btn-primary mx-2"><i class="bi bi-pencil-square"></i>Edit</a>                            <a asp-controller="MyRegistrationForm" asp-route-Id="@obj.Id" asp-action="Delete" class="btn btn-danger mx-2">Delete</a>                        </div>                    </td>                </tr>            </tbody>        }    </table>}else{}