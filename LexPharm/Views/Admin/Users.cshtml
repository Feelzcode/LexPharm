@model IEnumerable<LexPharm.Models.ApplicationUser>@{ ViewData["Title"] = "Application Page";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";}<div class="container">    <div class="row">        <div class="col-6">            <h1 class="text-primary">User's Profile'</h1>        </div>    </div>        @if (Model.Count() > 0)    {<table class="table-bordered table-striped" style="width:100%;">    <thead>        <tr>            <th>FirstName</th>            <th>UserName</th>            <th>Email</th>            <th>PhoneNumber</th>        </tr>    </thead>    @foreach (var usr in Model)    {<tbody>    <tr>        <td>@usr.FirstName</td>        <td>@usr.UserName</td>        <td>@usr.Email</td>        <td> @usr.PhoneNumber</td>        <td class="text-center">            <div class="w-75 btn-group" role="group">                <a asp-controller="MyRegistrationForm" asp-route-Id="@usr.Id" asp-action="Edit" class="btn btn-primary mx-2"><i class="bi bi-pencil-square"></i>Edit</a>                <a asp-controller="MyRegistrationForm" asp-route-Id="@usr.Id" asp-action="Delete" class="btn btn-danger mx-2">Delete</a>            </div>        </td>    </tr></tbody>}</table>}</div>